{
  "name": "zebrafish-facility-tracker",
  "author": "kevin shin",
  "version": "0.1.0",
  "main": "dist/main.js",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.2.9",
    "@electron/rebuild": "github:electron/rebuild",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "better-sqlite3": "^8.5.0",
    "electron": "^20.1.0",
    "framer-motion": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "run-script-os": "^1.1.6",
    "typescript": "^4.8.2",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "build": "run-script-os",
    "build:linux": "rm -r dist/ && ./node_modules/.bin/tsc && cd src && find -name '*.css' -exec cp --parents {} ../dist/ \\; && cd .. && ./src/server/init_sqlite3.sh",
    "build:macos": "rm -rf dist/ && ./node_modules/.bin/tsc && cd src && find . -name '*.css' -exec ditto {} ../dist/ \\; && cd .. && ./src/server/init_sqlite3.sh",
    "react": "react-scripts start",
    "electron": "electron .",
    "kill": "fuser -k 3000/tcp",
    "prestart": "npm run build",
    "start": "(BROWSER=none npm run react &) && sleep 3 && npm run electron",
    "poststart": "npm run kill",
    "test": "react-scripts test",
    "export": "./src/server/export_sqlite_to_csv.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.3",
    "webpack-cli": "^4.10.0"
  }
}
